<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<div class="container">
<button onclick="location.href='/'" class="btn btn-outline-primary mt-4">Home</button>
</div>
<h1 style="text-align: center;" class="m-3">New User</h1>
<div class="container">

	<form action="/save" method="POST">
        <div class="mb-3">
          <label class="form-label">Name:</label>
          <input type="text" name="nameu" class="form-control" required>
        </div>
        <div class="mb-3">
            <label class="form-label">Last Name:</label>
            <input type="text" name="lnameu" class="form-control" required>
          </div>
          <div class="mb-3">
            <label class="form-label">Email</label>
            <input type="email" name="email" class="form-control" required>
          </div>
          <div class="mb-3">
            <label class="form-label">Mobile</label>
            <input type="number" name="mobile" class="form-control">
          </div>
          <div class="mb-3">
            <label class="form-label">Birth</label>
            <input type="date" name="birth"class="form-control" required>
          </div>

       
        <button type="submit" class="btn btn-primary">Create</button>
      </form>
</div>

