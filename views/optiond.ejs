<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<div class="container">
<button onclick="location.href='/'" class="btn btn-outline-primary mt-4">Home</button>
</div>
<h1 style="text-align: center;" class="m-3">Do you really want to delete this user?</h1>
<h2 style="text-align: center;" class="m-2">If not, go back to home please</h2>
<% if (user) { %>
    <div class="container">
    
        <form>
            <div class="mb-3">
            <label class="form-label">Id:</label>
              <label><b><%= user.id %></b></label>
            </div>
            <div class="mb-3">
              <label class="form-label">Name:</label>
              <label><b><%= user.nameu %></b></label>
            </div>
            <div class="mb-3">
                <label class="form-label">Last Name:</label>
                <label><b><%= user.lnameu %></b></label>
            </div>
            <a href="/delete/<%= user.id %>" class="btn btn-danger">Delete</a>
          </form>
    </div>
    
    <% } %>
    

